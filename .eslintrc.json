{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "prefer-const": "error",
    "semi": "error",
    "no-extra-semi": "warn",
    "curly": "warn",
    // "quotes": ["error", "single", { "allowTemplateLiterals": true } ],
    "eqeqeq": "error"
  }
}
